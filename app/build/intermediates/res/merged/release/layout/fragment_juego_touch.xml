<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.adrin.juegoproyecto.JuegoActivity"
    android:id="@+id/absolutelayout_touch">

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_x="10dp"
        android:layout_y="306dp"
        android:src="@drawable/list_selector"
        android:background="#00ff9923"
        android:layout_above="@+id/noborrar"
        android:layout_alignEnd="@+id/imageView16" />

    <ImageButton
        android:layout_width="300dp"
        android:layout_height="120dp"
        android:text="New Button"
        android:id="@+id/button2"
        android:layout_x="231dp"
        android:layout_y="292dp"
        android:src="@drawable/list_selector"
        android:background="#00ff9923"
        android:layout_below="@+id/button"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="0dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:src="@drawable/bocadillobunny_listo_01" />

    <TextView
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_x="15dp"
        android:layout_y="10dp"
        android:text="0"
        android:textSize="25dp"
        android:textAlignment="center"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="40dp"
        android:textColor="#ffffff"
        android:layout_above="@+id/progressBar2"
        android:gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="40dp" />


    <TextView
    android:layout_width="200dp"
    android:layout_height="70dp"
    android:id="@+id/textView3"
    android:layout_x="191dp"
    android:layout_y="343dp"
    android:text="0"
    android:textSize="30dp"
    android:textIsSelectable="false"
    android:textAlignment="center"
    android:textStyle="bold"
        android:textColor="#ffffff"
        android:gravity="center"
        android:layout_below="@+id/timer"
        android:layout_alignStart="@+id/linearLayout"
        android:layout_marginTop="85dp"
        android:layout_marginBottom="20dp" />

    <SeekBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/progressBar2"
        android:thumb="@drawable/progreso_bunny_01"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:progressTint="#ffffff" />

    <ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView16"
    android:background="@drawable/progreso_zanahoria_01"
        android:layout_alignBottom="@+id/progressBar2"
        android:layout_alignParentEnd="true" />

    <TextView
    android:id="@+id/timer"
    android:paddingRight="10dip"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:textAlignment="center"
    android:textSize="25dp"
    android:textStyle="bold"
    android:layout_gravity="bottom"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/progressBar2"
        android:layout_alignParentStart="true" />

    <ImageView
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:id="@+id/noborrar"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="50dp" />

    <TextView
    android:layout_width="200dp"
    android:layout_height="70dp"
    android:id="@+id/textView4"
    android:layout_x="9dp"
    android:layout_y="225dp"
    android:text="0"
    android:textSize="30dp"
    android:textIsSelectable="false"
    android:textAlignment="center"
    android:textStyle="bold"
    android:layout_marginRight="4dp"
    android:textColor="#ffffff"
        android:layout_alignTop="@+id/button2"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="28dp"
        android:gravity="center" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="200dp"
        android:layout_height="110dp"
        android:id="@+id/linearLayout"
        android:background="@drawable/bocadillozanahoria_01"
        android:layout_marginRight="30dp"
        android:gravity="center"
        android:transitionGroup="false"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp">

        <TextView
        android:layout_width="67dp"
        android:layout_height="match_parent"
        android:id="@+id/textView17"
        android:text="0"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffffff"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="X"
            android:id="@+id/textView19"
            android:layout_alignTop="@+id/textView17"
            android:layout_toEndOf="@+id/textView17"
            android:layout_alignParentBottom="true"
            android:textColor="#ffffff"
            android:textSize="30dp"
            android:textAlignment="center"
            android:gravity="center" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/imageView32"
            android:src="@drawable/progreso_zanahoria_01"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/linearLayout"
            android:layout_marginLeft="5dp"
            android:tint="#ffffff" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="125dp"
        android:layout_height="70dp"
        android:id="@+id/linearLayout2"
        android:weightSum="1"
        android:background="@drawable/boton_verde_01"
        android:layout_alignTop="@+id/linearLayout"
        android:layout_alignEnd="@+id/textView2"
        android:layout_marginBottom="10dp"
        android:visibility="invisible"
        android:layout_toEndOf="@+id/linearLayout">

        <TextView
        android:layout_width="27dp"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/textView16"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
            android:textColor="#ffffff"
            android:layout_above="@+id/button"
            android:layout_centerHorizontal="true"
            android:singleLine="false"
            android:gravity="center"
            android:layout_weight="0.66"
            android:visibility="invisible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="X"
            android:id="@+id/textView21"
            android:layout_alignTop="@+id/textView20"
            android:layout_toEndOf="@+id/textView20"
            android:layout_alignParentBottom="true"
            android:textColor="#ffffff"
            android:textSize="30dp"
            android:textAlignment="center"
            android:gravity="center" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/imageView33"
            android:src="@drawable/progreso_bunny_01"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/linearLayout"
            android:visibility="invisible" />
    </LinearLayout>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView23"
        android:background="@drawable/corona"
        android:layout_above="@+id/button"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageVieweleccion1"
        android:layout_above="@+id/button"
        android:layout_toStartOf="@+id/imageView23"
        android:layout_marginRight="10dp" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageVieweleccion2"
        android:layout_above="@+id/button"
        android:layout_toEndOf="@+id/imageView23"
        android:layout_marginLeft="10dp" />

</RelativeLayout>
